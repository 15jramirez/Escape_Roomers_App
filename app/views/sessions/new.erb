<h1> Log in </h1>
<p> Don't have an account? <%= link_to "Sign Up", new_user_path %></p>
<%= form_for :session do |f| %> 
    <%= yield :form_errors %>
    <%= f.label "Username:" %>
    <%= f.text_field :username %>
    <%= f.label "Password:" %>
    <%= f.password_field :password %>
    <%= f.submit "log in" %>
<% end %>